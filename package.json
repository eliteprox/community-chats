{
  "name": "community-chats",
  "version": "1.0.0",
  "description": "Web3 Audio-Only Conference Application",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "node": "hardhat node",
    "gun-relay": "node gun-relay-server.js",
    "create-keystore": "ts-node scripts/create-keystore.ts",
    "deploy:local": "hardhat run scripts/deploy.ts --network localhost",
    "deploy:meeting-scheduler": "hardhat run scripts/deploy-meeting-scheduler.ts --network localhost",
    "deploy:arbitrum-testnet": "hardhat run scripts/deploy.ts --network arbitrumTestnet",
    "deploy:arbitrum": "hardhat run scripts/deploy.ts --network arbitrum",
    "deploy:arweave": "node scripts/deploy-arweave.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,sol,json,md}\""
  },
  "keywords": [
    "web3",
    "audio",
    "conference",
    "arbitrum",
    "ens",
    "arweave"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "hardhat": "^2.19.0",
    "prettier": "^3.1.0",
    "prettier-plugin-solidity": "^1.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@openzeppelin/contracts": "^5.4.0",
    "arweave": "^1.14.4",
    "dotenv": "^16.3.1",
    "gun": "^0.2020.1241"
  }
}
